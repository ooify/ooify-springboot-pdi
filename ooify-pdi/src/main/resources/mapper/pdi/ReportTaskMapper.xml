<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="me.ooify.pdi.mapper.ReportTaskMapper">
    
    <resultMap type="ReportTask" id="ReportTaskResult">
        <result property="id"    column="id"    />
        <result property="taskNo"    column="task_no"    />
        <result property="videoId"    column="video_id"    />
        <result property="videoUrl"    column="video_url"    />
        <result property="pipeInfo"    column="pipe_info"    />
        <result property="taskStatus"    column="task_status"    />
        <result property="queuePosition"    column="queue_position"    />
        <result property="retryCount"    column="retry_count"    />
        <result property="errorMessage"    column="error_message"    />
        <result property="flaskTaskId"    column="flask_task_id"    />
        <result property="reportUrl"    column="report_url"    />
        <result property="ossObjectKey"    column="oss_object_key"    />
        <result property="startTime"    column="start_time"    />
        <result property="endTime"    column="end_time"    />
        <result property="createBy"    column="create_by"    />
        <result property="isDeleted"    column="is_deleted"    />
        <result property="remark"    column="remark"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectReportTaskVo">
        select id, task_no, video_id, video_url, pipe_info, task_status, queue_position, retry_count, error_message, flask_task_id, report_url, oss_object_key, start_time, end_time, create_by, is_deleted, remark, create_time, update_time from report_task
    </sql>

    <select id="selectReportTaskList" parameterType="ReportTask" resultMap="ReportTaskResult">
        <include refid="selectReportTaskVo"/>
        <where>  
            <if test="taskNo != null  and taskNo != ''"> and task_no like concat('%', #{taskNo}, '%')</if>
            <if test="videoId != null "> and video_id like concat('%', #{videoId}, '%')</if>
            <if test="taskStatus != null "> and task_status = #{taskStatus}</if>
            <if test="params.beginStartTime != null and params.beginStartTime != '' and params.endStartTime != null and params.endStartTime != ''"> and start_time between #{params.beginStartTime} and #{params.endStartTime}</if>
            <if test="params.beginEndTime != null and params.beginEndTime != '' and params.endEndTime != null and params.endEndTime != ''"> and end_time between #{params.beginEndTime} and #{params.endEndTime}</if>
            <if test="createBy != null  and createBy != ''"> and create_by like concat('%', #{createBy}, '%')</if>
            <if test="isDeleted != null "> and is_deleted = #{isDeleted}</if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''"> and create_time between #{params.beginCreateTime} and #{params.endCreateTime}</if>
        </where>
    </select>
    
    <select id="selectReportTaskById" parameterType="Long" resultMap="ReportTaskResult">
        <include refid="selectReportTaskVo"/>
        where id = #{id}
    </select>

    <insert id="insertReportTask" parameterType="ReportTask" useGeneratedKeys="true" keyProperty="id">
        insert into report_task
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="taskNo != null">task_no,</if>
            <if test="videoId != null">video_id,</if>
            <if test="videoUrl != null and videoUrl != ''">video_url,</if>
            <if test="pipeInfo != null and pipeInfo != ''">pipe_info,</if>
            <if test="taskStatus != null">task_status,</if>
            <if test="queuePosition != null">queue_position,</if>
            <if test="retryCount != null">retry_count,</if>
            <if test="errorMessage != null">error_message,</if>
            <if test="flaskTaskId != null">flask_task_id,</if>
            <if test="reportUrl != null">report_url,</if>
            <if test="ossObjectKey != null">oss_object_key,</if>
            <if test="startTime != null">start_time,</if>
            <if test="endTime != null">end_time,</if>
            <if test="createBy != null">create_by,</if>
            <if test="isDeleted != null">is_deleted,</if>
            <if test="remark != null">remark,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="taskNo != null">#{taskNo},</if>
            <if test="videoId != null">#{videoId},</if>
            <if test="videoUrl != null and videoUrl != ''">#{videoUrl},</if>
            <if test="pipeInfo != null and pipeInfo != ''">#{pipeInfo},</if>
            <if test="taskStatus != null">#{taskStatus},</if>
            <if test="queuePosition != null">#{queuePosition},</if>
            <if test="retryCount != null">#{retryCount},</if>
            <if test="errorMessage != null">#{errorMessage},</if>
            <if test="flaskTaskId != null">#{flaskTaskId},</if>
            <if test="reportUrl != null">#{reportUrl},</if>
            <if test="ossObjectKey != null">#{ossObjectKey},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="isDeleted != null">#{isDeleted},</if>
            <if test="remark != null">#{remark},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateReportTask" parameterType="ReportTask">
        update report_task
        <trim prefix="SET" suffixOverrides=",">
            <if test="taskNo != null">task_no = #{taskNo},</if>
            <if test="videoId != null">video_id = #{videoId},</if>
            <if test="videoUrl != null and videoUrl != ''">video_url = #{videoUrl},</if>
            <if test="pipeInfo != null and pipeInfo != ''">pipe_info = #{pipeInfo},</if>
            <if test="taskStatus != null">task_status = #{taskStatus},</if>
            <if test="queuePosition != null">queue_position = #{queuePosition},</if>
            <if test="retryCount != null">retry_count = #{retryCount},</if>
            <if test="errorMessage != null">error_message = #{errorMessage},</if>
            <if test="flaskTaskId != null">flask_task_id = #{flaskTaskId},</if>
            <if test="reportUrl != null">report_url = #{reportUrl},</if>
            <if test="ossObjectKey != null">oss_object_key = #{ossObjectKey},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="endTime != null">end_time = #{endTime},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="isDeleted != null">is_deleted = #{isDeleted},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteReportTaskById" parameterType="Long">
        delete from report_task where id = #{id}
    </delete>

    <delete id="deleteReportTaskByIds" parameterType="String">
        delete from report_task where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>